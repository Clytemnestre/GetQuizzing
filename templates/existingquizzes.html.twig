{% extends "educatorsmaster.html.twig" %}
{% block content %}
        <h2>
            You have already created the quiz (zes) below
        </h2><br>
        <table border="1">
            {% for quiz in quizList %}
            <tr>
                <td>{{quiz.title}}</td>
                <td><a href="/questions/{{quiz.ID}}">Add / Delete / Modify</a></td>
                <td><a href="/invites/{{quiz.ID}}">send to learners</a></td>
                <td><a href="/seeresults/{{quiz.ID}}">see results</a></td>
                <td><a href="/deletequiz/{{quiz.ID}}">delete</a></td>
            </tr>
            {% endfor %}
        </table>
{% endblock %}

